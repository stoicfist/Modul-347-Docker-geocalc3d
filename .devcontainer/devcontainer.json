{
  "name": "Geocalc Fullstack DevContainer",
  "dockerComposeFile": ["../docker/docker-compose.yml"],
  "service": "devcontainer",
  "workspaceFolder": "/workspace",
  "forwardPorts": [4200, 8080, 8081, 5433, 5050],
  "postCreateCommand": "npm install --prefix geocalc-frontend",
  "extensions": [
    "vscjava.vscode-java-pack",
    "angular.ng-template",
    "dbaeumer.vscode-eslint"
  ]
}
